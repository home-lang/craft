{
  "name": "@craft/svelte",
  "version": "1.3.0",
  "description": "Svelte bindings for Craft framework",
  "svelte": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "svelte": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest"
  },
  "keywords": ["craft", "svelte", "stores", "actions", "native"],
  "author": "Craft Team",
  "license": "MIT",
  "peerDependencies": {
    "svelte": "^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "svelte": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}

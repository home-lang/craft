{
  "name": "craft-benchmarks",
  "version": "1.0.0",
  "description": "Performance benchmarks comparing Craft to Electron and Tauri",
  "type": "module",
  "scripts": {
    "bench": "bun run-all.ts",
    "bench:craft": "bun craft/startup.bench.ts",
    "bench:electron": "bun electron/startup.bench.ts",
    "bench:tauri": "bun tauri/startup.bench.ts",
    "bench:memory": "bun run-memory.ts",
    "bench:cpu": "bun run-cpu.ts",
    "bench:all": "bun run bench && bun run bench:memory && bun run bench:cpu"
  },
  "dependencies": {
    "mitata": "^1.0.10"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
